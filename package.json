{
  "name": "checkpoint",
  "version": "1.0.0",
  "description": "This is a sample check point application",
  "license": "MIT",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build": "tsc && node copy-dir && node minify",
    "watch": "tsc -w",
    "format": "prettier --write \"**/*.ts\"",
    "start": "export NODE_ENV=test && ./node_modules/.bin/forever -m 5 main.js",
    "start:raw": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev-win": "set NODE_ENV=development && nodemon -e ts  --watch src",
    "start:dev": "export NODE_ENV=development && nodemon -e ts  --watch src",
    "prestart:test-win": "rmdir /S dist && tsc && node copy-dir && node minify",
    "start:test-win": "set NODE_ENV=test && node dist/main.js",
    "prestart:test": "rm -rf dist && tsc && node copy-dir && node minify",
    "start:test": "export NODE_ENV=test && node dist/main.js",
    "prestart:prod": "rm -rf dist && tsc && node copy-dir && node minify",
    "start:prod": "export NODE_ENV=production && node dist/main.js",
    "db:seed:all": "export NODE_ENV=development && node seeders --seed",
    "db:seed:all-win": "set NODE_ENV=development && node seeders --seed",
    "db:seed:undo": "node seeders --undo",
    "db:seed:prod:all": "export NODE_ENV=production && node seeders --seed",
    "db:seed:prod:all-win": "set NODE_ENV=production && node seeders --seed",
    "db:seed:test:all": "export NODE_ENV=test && node seeders --seed",
    "db:seed:test:all-win": "set NODE_ENV=test && node seeders --seed",
    "env:test": "export NODE_ENV=test",
    "env:dev": "export NODE_ENV=development",
    "env:prod": "export NODE_ENV=production",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/mongoose": "^5.1.0",
    "@nestjs/swagger": "1.2.2",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/websockets": "^4.6.6",
    "@types/nodemailer": "^4.6.0",
    "@types/nodemailer-direct-transport": "^1.0.31",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "axios": "^0.18.0",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.8.5",
    "cli-color": "^1.3.0",
    "cluster": "^0.7.7",
    "consolidate": "^0.15.1",
    "express-fileupload": "^0.4.0",
    "faker": "^4.1.0",
    "forever": "^0.15.3",
    "fs-extra": "^6.0.1",
    "fs-jetpack": "^1.3.0",
    "google-distance-matrix": "^1.1.1",
    "jimp": "^0.2.28",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "mailgun-js": "^0.18.0",
    "minifier": "^0.8.1",
    "mongoose": "^5.2.7",
    "mongoose-delete": "^0.4.0",
    "mongoose-double": "0.0.1",
    "mustache": "^2.3.0",
    "nodemailer": "^4.6.4",
    "password-hash": "^1.2.2",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.10",
    "swig": "^1.4.2",
    "typescript": "^2.8.3",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.6.17",
    "@types/supertest": "^2.0.4",
    "jest": "^21.2.1",
    "nodemon": "^1.17.4",
    "prettier": "^1.12.1",
    "supertest": "^3.1.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.3.2",
    "tslint": "5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Procaseycash/checkpoint.git"
  },
  "keywords": [
    "Notification",
    "Angular",
    "Alert",
    "Pop"
  ],
  "author": "Kazeem Olanipekun <kezyolanipekun@gmail.com>",
  "contributors": [
    "Kazeem Olanipekun <kezyolanipekun@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/Procaseycash/checkpoint/issues"
  },
  "homepage": "https://github.com/Procaseycash/checkpoint#README"
}
